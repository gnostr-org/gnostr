{
	"$schema": "https://json.schemastore.org/web-manifest-combined.json",
	"name": "gnostr.org",
	"short_name": "gnostr",
	"start_url": "..",
	"display": "standalone",
	"background_color": "#0f0f0f",
	"theme_color": "#0f0f0f",
	"description": "A blue bird Nostr experience.",
	"icons": [
		{
			"src": "../images/icon.svg",
			"sizes": "any",
			"type": "image/svg+xml"
		},
		{
			"src": "../images/icon-maskable.svg",
			"sizes": "any",
			"type": "image/svg+xml",
			"purpose": "maskable any"
		}, 
		{
			"src": "splash.png",
			"sizes": "512x512",
			"type": "image/png"
		}
	]
}
